#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hariharan/Desktop/work/my-future/new/push this/test_project/node_modules/.pnpm/next-sitemap@4.2.3_next@15.5.4_@playwright+test@1.56.0_babel-plugin-react-compiler@19.1_5a73f6c832259a0f0dd158b900d013de/node_modules/next-sitemap/bin/node_modules:/Users/hariharan/Desktop/work/my-future/new/push this/test_project/node_modules/.pnpm/next-sitemap@4.2.3_next@15.5.4_@playwright+test@1.56.0_babel-plugin-react-compiler@19.1_5a73f6c832259a0f0dd158b900d013de/node_modules/next-sitemap/node_modules:/Users/hariharan/Desktop/work/my-future/new/push this/test_project/node_modules/.pnpm/next-sitemap@4.2.3_next@15.5.4_@playwright+test@1.56.0_babel-plugin-react-compiler@19.1_5a73f6c832259a0f0dd158b900d013de/node_modules:/Users/hariharan/Desktop/work/my-future/new/push this/test_project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hariharan/Desktop/work/my-future/new/push this/test_project/node_modules/.pnpm/next-sitemap@4.2.3_next@15.5.4_@playwright+test@1.56.0_babel-plugin-react-compiler@19.1_5a73f6c832259a0f0dd158b900d013de/node_modules/next-sitemap/bin/node_modules:/Users/hariharan/Desktop/work/my-future/new/push this/test_project/node_modules/.pnpm/next-sitemap@4.2.3_next@15.5.4_@playwright+test@1.56.0_babel-plugin-react-compiler@19.1_5a73f6c832259a0f0dd158b900d013de/node_modules/next-sitemap/node_modules:/Users/hariharan/Desktop/work/my-future/new/push this/test_project/node_modules/.pnpm/next-sitemap@4.2.3_next@15.5.4_@playwright+test@1.56.0_babel-plugin-react-compiler@19.1_5a73f6c832259a0f0dd158b900d013de/node_modules:/Users/hariharan/Desktop/work/my-future/new/push this/test_project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next-sitemap/bin/next-sitemap.mjs" "$@"
else
  exec node  "$basedir/../next-sitemap/bin/next-sitemap.mjs" "$@"
fi
